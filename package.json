    "next": "13.5.6",
    "next-auth": "^4.24.11",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.16.0",
    "resend": "^6.0.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@playwright/test": "^1.48.2",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.4.3",
