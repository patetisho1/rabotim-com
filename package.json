    "analyze:build": "npm run build && npm run analyze",
    "setup-email": "node scripts/setup-email.js"
  },
  "dependencies": {
    "@react-email/render": "^1.2.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.54.0",
